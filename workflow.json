        "jsCode": "const rawText = $input.first().json.content;\n\n/**\n * L√†m s·∫°ch n·ªôi dung vƒÉn b·∫£n\n */\nfunction cleanContent(text) {\n    if (typeof text !== 'string' || !text) {\n        return \"\";\n    }\n    \n    // X√≥a c√°c c·ª•m c√≥ c·∫•u tr√∫c (number) v√≠ d·ª•: (1), (2), (3), (10), etc.\n    text = text.replace(/\\(\\d+\\)/g, '');\n    \n    // X√≥a c√°c c·ª•m c√≥ c·∫•u tr√∫c \"xem h√¨nh ... b√™n d∆∞·ªõi\"\n    text = text.replace(/\\(xem h√¨nh[^)]*b√™n d∆∞·ªõi\\)/gi, '');\n    \n    // X√≥a c√°c c·ª•m c√≥ c·∫•u tr√∫c \"H√¨nh X. M√¥ t·∫£\"\n    text = text.replace(/H√¨nh\\s+\\d+\\.\\s*[^\\n]*/gi, '');\n    \n    // Lo·∫°i b·ªè k√Ω t·ª± ƒë·∫∑c bi·ªát kh√¥ng c·∫ßn thi·∫øt\n    text = text.replace(/[^\\w\\s\\u00C0-\\u024F\\u1E00-\\u1EFF.,!?():\\-‚Üí‚Ä¢\\d]/g, ' ');\n    \n    // Chu·∫©n h√≥a k√Ω t·ª± ƒë·∫∑c bi·ªát\n    text = text.replace(/‚Üí/g, ' -> ');\n    text = text.replace(/‚Ä¢/g, '- ');\n    text = text.replace(/[\\u2022\\u2023\\u25E6\\u2043\\u2219]/g, '- '); // C√°c lo·∫°i bullet points\n    \n    // Chu·∫©n h√≥a kho·∫£ng tr·∫Øng (GI·ªÆ line breaks)\n    text = text.replace(/[ \\t]+/g, ' '); // Ch·ªâ nhi·ªÅu spaces/tabs th√†nh 1, GI·ªÆ \\n\n    text = text.replace(/\\t/g, ' ');   // Tab th√†nh space\n    \n    // Chu·∫©n h√≥a xu·ªëng d√≤ng\n    text = text.replace(/\\n\\s*\\n\\s*\\n+/g, '\\n\\n'); // Nhi·ªÅu d√≤ng tr·ªëng th√†nh 2 d√≤ng\n    text = text.replace(/\\n\\s+/g, '\\n'); // Lo·∫°i b·ªè space ƒë·∫ßu d√≤ng\n    \n    // Lo·∫°i b·ªè d√≤ng tr·ªëng ·ªü ƒë·∫ßu v√† cu·ªëi\n    text = text.trim();\n    \n    return text;\n}\n\n/**\n * Chu·∫©n h√≥a c·∫•u tr√∫c vƒÉn b·∫£n\n */\nfunction normalizeStructure(text) {\n    const lines = text.split('\\n');\n    const cleanedLines = [];\n    \n    for (let i = 0; i < lines.length; i++) {\n        let line = lines[i].trim();\n        if (!line) {\n            continue;\n        }\n        \n        // T√°ch th√¥ng tin li√™n h·ªá l·∫∑p l·∫°i\n        const contactKeywords = ['1900 88 66 52', 'hot